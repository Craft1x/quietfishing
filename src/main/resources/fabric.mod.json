{
  "schemaVersion": 1,
  "id": "quietfishing",
  "version": "${version}",

  "name": "Quiet fishing mod",
  "description": "This mod removes the sound of other players' fishing bobbers",
  "authors": [
    "Craft1x"
  ],
  "contact": {
    "homepage": "www.fiverr.com/users/craft1x",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/quietfishing/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fabricmc.quietfishing.QuietFishing"
    ]
  },
  "mixins": [
    "quietfishing.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
